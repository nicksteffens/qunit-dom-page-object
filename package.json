{
  "name": "qunit-dom-page-object",
  "version": "1.0.0",
  "description": "Qunit-DOM plugin to allow use of page-objects",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "exports": "dist/index.modern.js",
  "module": "dist/index.module.js",
  "keywords": [
    "page-objects",
    "qunit-dom",
    "qunit-plugin",
    "qunit",
    "testing"
  ],
  "scripts": {
    "build": "microbundle",
    "test": "qunit tests",
    "prepare": "husky install",
    "prepublishOnly": "microbundle"
  },
  "author": "",
  "license": "MIT",
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  },
  "lint-staged": {
    "*.{ts,json,md,yml}": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0
      ]
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@movable/eslint-config": "^2.0.0",
    "@movable/prettier-config": "^1.0.0",
    "@types/qunit": "^2.11.2",
    "eslint": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "microbundle": "^0.14.1",
    "prettier": "^2.4.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "typescript": "^4.4.4"
  },
  "packageManager": "yarn@3.1.0"
